{
  "name": "node-check-console-plugin",
  "version": "1.0.0",
  "description": "OpenShift Console Plugin for Node Check Operator",
  "main": "dist/index.js",
  "consolePlugin": {
    "name": "node-check-console-plugin",
    "version": "1.0.0",
    "displayName": "Node Check Operator",
    "description": "Plugin for monitoring bare metal nodes",
    "exposedModules": {
      "NodeCheckOverview": "./src/pages/NodeCheckOverview"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  },
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack serve --mode=development",
    "dev:mock": "node mock-api-server.js",
    "dev:all": "concurrently \"npm run dev:mock\" \"npm run dev\"",
    "test:local": "./test-local-plugin.sh",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@patternfly/react-charts": "^8.4.0",
    "@patternfly/react-core": "^6.2.2",
    "@patternfly/react-icons": "^6.2.2",
    "@patternfly/react-table": "^6.2.2",
    "victory-area": "^37.3.6",
    "victory-axis": "^37.3.6",
    "victory-bar": "^37.3.6",
    "victory-box-plot": "^37.3.6",
    "victory-chart": "^37.3.6",
    "victory-core": "^37.3.6",
    "victory-create-container": "^37.3.6",
    "victory-cursor-container": "^37.3.6",
    "victory-group": "^37.3.6",
    "victory-legend": "^37.3.6",
    "victory-line": "^37.3.6",
    "victory-pie": "^37.3.6",
    "victory-scatter": "^37.3.6",
    "victory-stack": "^37.3.6",
    "victory-tooltip": "^37.3.6",
    "victory-voronoi-container": "^37.3.6"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@openshift-console/dynamic-plugin-sdk": "1.4.0",
    "@openshift-console/dynamic-plugin-sdk-webpack": "1.1.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "5.75.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
